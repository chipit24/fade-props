{
  "name": "fade-props",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.4.0"
  },
  "description": "A ReactJS component that seamlessly fades between the props it is provided; it fades one child out, removes it, then fades the next child in.",
  "keywords": [
    "react-component",
    "transition",
    "fade"
  ],
  "main": "index.js",
  "module": "src/fade-props.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "webpack -p",
    "prepublishOnly": "yarn build && yarn test",
    "preversion": "yarn build && yarn test",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "repository": "https://github.com/chipit24/fade-props",
  "author": "Robert Komaromi <robertkomaromi@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^20.0.4",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.6.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.3.3"
  }
