{
  "name": "fade-props",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.4.0"
  },
  "description": "A ReactJS component that seamlessly fades between the props it is provided; it fades one child out, removes it, then fades the next child in.",
  "keywords": [
    "react-component",
    "transition",
    "fade"
  ],
  "main": "FadeProps.js",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --colors --content-base demo",
    "build": "webpack",
    "prepublish": "npm test && npm run build",
    "preversion": "npm test && npm run build",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chipit24/fade-props"
  },
  "jest": {
    "automock": false
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  },
  "author": "Robert Komaromi <robertkomaromi@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "jest-cli": "^14.1.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.0.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.3.3",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "react": "^15.0.1"
  }
}
